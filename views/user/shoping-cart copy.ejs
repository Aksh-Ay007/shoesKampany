                        </div>
                    <% } else { %>
                        <div class="wrap-table-shopping-cart">
                            <table class="table table-shopping-cart">
                                <thead class="table_head">
                                    <tr>
                                        <th class="column-1">Product</th>
                                        <th class="column-2">Name</th>
										<th class="column-6">Remove</th>
                                        <th class="column-3">Price</th>
                                        <th class="column-4">Quantity</th>
                                        <th class="column-5">Total</th>
                                       
                                    </tr>
                                </thead>
                                <tbody>
                                    <% cart.items.forEach((item, index) => { %>
                                        <tr class="table_row">
                                            <td class="column-1">
                                                <div class="how-itemcart1">
                                                    <img src="<%= item.productId.images[0] %>" alt="Product Image" class="img-thumbnail">
                                                </div>
                                            </td>
                                            <td class="column-2" style="font-weight: bold; font-size: 1em; color:black;">
                                                <%= item.productId.product_name %>
                                                <% if (item.productId.stock <= 0) { %>
                                                    <p style="color: red;">Out of Stock</p>
                                                <% } %>
                                            </td>
                                            <td class="column-3" style="font-weight: bold; font-size: 1em; color:black;">
                                                ₹<%= item.productId.price %>
                                                <% if (item.productId.offerPrice && item.productId.offerPrice < item.productId.price) { %>
                                                    <br>Offer: ₹<%= item.productId.offerPrice %> 
                                                    (<%= Math.round(((item.productId.price - item.productId.offerPrice) / item.productId.price) * 100) %>% off)
                                                <% } %>
                                            </td>
                                            <td class="column-4">
                                                <div class="wrap-num-product flex-w m-l-auto m-r-0">
                                                    <button type="button" class="quantity-right-plus btn cl8" onclick="quantity('dec', '<%= item.productId._id %>')" data-type="minus" <%= item.productId.stock <= 0 ? 'disabled' : '' %>>
                                                        <i class="fs-16 zmdi zmdi-minus"></i>
                                                    </button>
                                                    <input class="mtext-104 cl3 txt-center num-product" type="number" name="num-product1" id="quantity_<%= item.productId._id %>" value="<%= item.quantity %>" max="5" disabled>
                                                    <button type="button" class="quantity-right-plus btn cl8" onclick="quantity('inc', '<%= item.productId._id %>')" data-type="plus" <%= item.productId.stock <= 0 ? 'disabled' : '' %>>
                                                        <i class="fs-16 zmdi zmdi-plus"></i>
                                                    </button>
                                                </div>
                                            </td>
                                            <td class="column-5" style="font-weight: bold; font-size: 1em; color:black;">
                                                <% if (item.productId.offerPrice && item.productId.offerPrice < item.productId.price) { %>
                                                    ₹<%= item.productId.offerPrice * item.quantity %>
                                                <% } else { %>
                                                    ₹<%= item.productId.price * item.quantity %>
                                                <% } %>
                                            </td>
                                            <td class="column-6">
                                                <!-- Delete button with font-awesome icon -->
                                                <button class="delete-btn" data-id="<%= item.productId._id %>">
                                                    <i class="bi bi-trash-fill"></i> <!-- Adjust icon class as needed -->
                                                </button>
                                            </td>
                                        </tr>
                                    <% }) %>
                                </tbody>
                            </table>
                        </div>
                    <% } %>
                </div>
            </div>

            <% if (cart && cart.items && cart.items.length > 0) { %>
                <div class="m-lr-auto m-b-50">
                    <div class="bor10 p-lr-40 p-t-30 p-b-40 p-lr-15-sm container-box-2">
                        <h4 class="mtext-109 cl2 p-b-30">Cart Totals</h4>
                        <div class="totals-row">
                            <div class="totals-label">Discount Amount:</div>
                            <div class="totals-value totals-discount">
                                <% let discountAmount = 0; %>
                                <% cart.items.forEach(item => { %>
                                    <% if (item.productId.offerPrice && item.productId.offerPrice < item.productId.price) { %>
                                        <% discountAmount += (item.productId.price - item.productId.offerPrice) * item.quantity; %>
                                    <% } %>
                                <% }); %>
                                ₹<%= discountAmount %>
                            </div>
                        </div>
                        <div class="totals-row">
                            <div class="totals-label">Total Amount:</div>
                            <div class="totals-value totals-total">₹<%= cart.totalAmount %></div>
                        </div>
                        <a href="/checkout" class="checkout-btn">Proceed to Checkout</a>
                    </div>
                </div>
            <% } %>
        </div>
    </div>
</form>








