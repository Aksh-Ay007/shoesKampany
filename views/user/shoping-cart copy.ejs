<script>


const quantity = async (type, productId) => {
    console.log('Type:', type);
    console.log('Product ID:', productId);
    try {
        const res = await fetch(`/quantity/${productId}?action=${type}`, {
            method: 'GET',
            headers: {
                'Content-Type': 'application/json'
            },
        });

        const data = await res.json();
        
        if (!res.ok) {
            throw new Error(data.error || 'Failed to update quantity');
        }

        console.log(data.message); // log the response message

        const quantityInput = document.getElementById(`quantity_${productId}`);
        const notification = document.getElementById('notification');
        
        if (data.message === 'Quantity updated successfully') {
            if (type === 'inc') {
                quantityInput.classList.add('highlight-increase');
            } else if (type === 'dec') {
                quantityInput.classList.add('highlight-decrease');
            }
            
            setTimeout(() => {
                quantityInput.classList.remove('highlight-increase');
                quantityInput.classList.remove('highlight-decrease');
                window.location.reload(); // Reload the page after the delay
            }, 500);
        } else if (data.error === 'Maximum quantity per person is 5') {
            notification.innerText = 'You can only purchase up to 5 units of this product.';
            notification.style.display = 'block';
            setTimeout(() => {
                notification.style.display = 'none';
            }, 5000);
        } else if (data.error === 'Product is out of stock') {
            notification.innerText = 'This product is currently out of stock.';
            notification.style.display = 'block';
            setTimeout(() => {
                notification.style.display = 'none';
            }, 5000);
        } else {
            notification.innerText = data.error || 'Unknown error occurred';
            notification.style.display = 'block';
            setTimeout(() => {
                notification.style.display = 'none';
            }, 5000);
        }

    } catch (error) {
        console.error(error);
        const notification = document.getElementById('notification');
        notification.innerText = error.message;
        notification.style.display = 'block';
        setTimeout(() => {
            notification.style.display = 'none';
        }, 5000);
    }
};

</script>

<script>
	$(document).ready(function () {
    // Attach click event listener to delete buttons
    $('.delete-btn').on('click', function (e) {
        e.preventDefault(); // Prevent default button behavior

        // Get the data-id attribute of the delete button
        const productId = $(this).data('id');

        // Show confirmation dialog
        Swal.fire({
            title: "Are you sure?",
            text: "Do you really want to delete this item?",
            icon: "warning",
            showCancelButton: true,
            confirmButtonText: "Yes, delete it!",
            cancelButtonText: "No, cancel",
            reverseButtons: true
        }).then((result) => {
            // Check if user confirmed deletion
            if (result.isConfirmed) {
                // Send AJAX request to delete the item
                $.ajax({
                    url: `/delete/${productId}`, // Replace with the correct endpoint
                    method: 'DELETE',
                    success: function (response) {
                        // If deletion is successful, remove the corresponding table row
                        $(e.target).closest('tr').remove();

                        // Show success message
                        Swal.fire({
                            title: "Deleted!",
                            text: "Item has been deleted successfully",
                            icon: "success",
                            confirmButtonText: "OK"
                        });

                        // Check if the cart is empty
                        if ($('.table_row').length === 0) {
                            // If the cart is empty, refresh the page
                            location.reload();
                        }
                    },
                    error: function (xhr, status, error) {
                        console.error(error); // Log any errors to the console

                        // Show error message
                        Swal.fire({
                            title: 'Error!',
                            text: 'Failed to delete item',
                            icon: 'error',
                            confirmButtonText: 'OK'
                        });
                    }
                });
            }
        });
    });
});

</script>






