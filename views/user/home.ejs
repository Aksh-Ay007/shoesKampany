        body {
            font-family: Arial, sans-serif;
            margin: 0;
            padding: 0;
        }
        .scroll-container {
            position: sticky;
            top: 0;
            width: 100%;
            height: 30px; /* Adjust the height as needed */
            overflow: hidden;
            background: transparent;
            color: blue;
            font-size: small;
            text-align: center;
            z-index: 1000; /* Ensure it stays on top */
        }
        .scroll-content {
            display: inline-block;
            white-space: nowrap;
            animation: scrollX 10s linear infinite;
        }
        @keyframes scrollX {
            0% { transform: translateX(100%); }
            100% { transform: translateX(-100%); }
        }
        .hidden {
            display: none;
        }
    </style>
		<style>
        .imageZoom {
            width: 100%;
            height: 250px;
            position: relative;
            overflow: hidden;
        }
        .imageZoom img {
            width: 100%;
            height: 100%;
            object-fit: cover;
            object-position: 0 0;
        }
        .imageZoom::after {
            display: var(--display);
            content: '';
            width: 100%;
            height: 100%;
            background-color: black;
            background-image: var(--url);
            background-size: 200%;
            background-position: var(--zoom-x) var(--zoom-y);
            position: absolute;
            left: 0;
            top: 0;
        }
		

