<section class="bg0 p-t-23 p-b-140">
    <div class="container">
        <div class="p-b-10">
            <h3 class="ltext-103 cl5">Our Products</h3>
        </div>
        <div class="product-grid">
            <% allProduct.forEach(product => { %>
            <div class="product-item">
                <div class="product-image">
                    <img src="<%= product.images[0] %>" alt="IMG-PRODUCT">
                </div>
                <div class="product-details">
                    <a href="/product?id=<%= product._id %>" class="product-name"><%= product.product_name %></a>
                    <div class="product-price">
                        <% if (product.discountPercentage) { %>
                            <span class="original-price">&#x20a8; <%= product.originalPrice %></span>
                            <span class="discount-percentage">(<%= product.discountPercentage %>% off)</span><br>
                            <span class="offer-price">Offer Price: &#x20a8; <%= product.price %></span>
                        <% } else { %>
                            <span class="regular-price">&#x20a8; <%= product.price %></span>
                        <% } %>
                    </div>
                    <% if (product.stock > 0) { %>
                        <span class="stock-<%= product.stock < 10 ? 'low' : 'available' %>">
                            <%= product.stock < 10 ? 'Only few left' : 'In Stock (' + product.stock + ' left)' %>
                        </span>
                    <% } else { %>
                        <span class="stock-unavailable">Out of Stock</span>
                    <% } %>
                    <!-- Star Rating -->
                    <div class="product-rating">
                        <% const fullStars = Math.floor(product.averageRating); %>
                        <% const hasHalfStar = product.averageRating % 1 >= 0.5; %>
                        <% for(let i = 1; i <= 5; i++) { %>
                            <% if(i <= fullStars) { %>
                                <i class="fas fa-star filled"></i>
                            <% } else if(i === fullStars + 1 && hasHalfStar) { %>
                                <i class="fas fa-star-half-alt filled"></i>
                            <% } else { %>
                                <i class="far fa-star"></i>
                            <% } %>
                        <% } %>
                        <span class="rating-count">(<%= product.totalReviews %> Reviews)</span>
                    </div>

                    <% if(isUser){ %>
                    <div class="product-actions">
                        <button class="btn-addwish" data-tooltip="Add to wishlist" data-pdt-id="<%= product._id %>">
                            <i class="zmdi zmdi-favorite"></i>
                        </button>
                        <% if (product.stock > 0) { %>
                        <button class="btn-addcart" data-tooltip="Add to Cart" data-pdt-id="<%= product._id %>">
                            <i class="zmdi zmdi-shopping-cart"></i>
                        </button>
                        <% } else { %>
                        <!-- Disable cart button for out of stock products -->
                        <button class="btn-addcart" data-tooltip="Out of Stock" disabled>
                            <i class="zmdi zmdi-shopping-cart"></i>
                        </button>
                        <% } %>
                    </div>
					  <% } %>
                </div>
            </div>
            <% }) %>
        </div>
